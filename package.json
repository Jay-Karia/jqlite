{
  "name": "@jqlite/core",
  "version": "1.0.0",
  "description": "The core engine of the query language",
  "packageManager": "pnpm@10.6.5",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --outdir=dist --format=esm --minify",
    "build:watch": "pnpm run build --watch",
    "start": "node dist/index.js",
    "start:ts": "npx tsx src/index.ts",
    "dev": "node --watch dist/index.js",
    "lint": "npx eslint .",
    "lint:src": "npx eslint src",
    "format": "npx prettier . --write",
    "format:src": "npx prettier src --write",
    "test:src": "vitest",
    "coverage:src": "vitest run --coverage"
  },
  "keywords": [
    "query-language",
    "json",
    "engine",
    "core",
    "fast"
  ],
  "author": "Jay-Karia",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "3.0.5",
    "esbuild": "^0.25.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^15.14.0",
    "prettier": "3.4.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.6",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.23.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@jqlite/core": "link:",
    "core": "link:../../.local/share/pnpm/global/5/node_modules/@jqlite/core"
  },
  "pnpm": {
    "overrides": {
      "@jqlite/core": "link:",
      "core": "link:../../.local/share/pnpm/global/5/node_modules/@jqlite/core"
    }
  }
}
