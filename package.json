{
  "name": "@jqlite/core",
  "version": "0.0.1",
  "description": "The core engine of the query language",
  "packageManager": "pnpm@10.8.0",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --outdir=dist --format=esm --minify",
    "build:watch": "pnpm run build --watch",
    "start": "node dist/index.js",
    "start:ts": "tsx src/index.ts",
    "dev": "node --watch dist/index.js",
    "lint": "npx eslint .",
    "lint:src": "npx eslint src",
    "format": "npx prettier . --write",
    "format:src": "npx prettier src --write",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "play": "nodemon -e ts -w . -x tsx playground.ts"
  },
  "keywords": [
    "query-language",
    "json",
    "engine",
    "core",
    "fast"
  ],
  "author": "Jay-Karia",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.13.1",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "3.1.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "nodemon": "^3.1.9",
    "prettier": "3.4.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.6",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.23.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitepress": "^1.6.3",
    "vitest": "^3.1.1"
  }
}
